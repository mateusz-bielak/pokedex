{
    "name": "pokedex",
    "version": "1.0.0",
    "author": "Mateusz Bielak",
    "main": "app.js",
    "scripts": {
        "dev": "parcel index.html --open",
        "build": "parcel build index.html",
        "lint": "eslint \"**/*.{js,jsx}\" -f codeframe --color",
        "lint:fix": "npm run lint --fix",
        "prettier-fix": "prettier --config .prettierrc --write ./src/**/*.{ts,tsx,js,jsx}"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    },
    "devDependencies": {
        "eslint": "^5.15.3",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "husky": "^1.3.1",
        "parcel-bundler": "^1.12.3",
        "parcel-plugin-eslint": "^1.0.7",
        "prettier": "^1.16.4"
    },
    "dependencies": {
        "prop-types": "^15.7.2",
        "react": "^16.8.5",
        "react-dom": "^16.8.5"
    },
    "license": "ISC"
}
